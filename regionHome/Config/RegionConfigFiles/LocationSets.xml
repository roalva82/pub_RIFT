<locationSets version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/locationSets.xsd">
	<!-- All Location Sets Built from csv and shp files. See MapLayerFiles for source-->
	<!--- Water Survey of Canada Hydrometric Stations - Available at https://dd.weather.gc.ca/hydrometric/doc/ -->
	<locationSet id="WSCStations">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
		<csvFile>
			<file>hydrometric_StationList.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%ID%</id>
			<name>%Name / Nom%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<attribute id="ID" name="Station Code">
				<text>%ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Prov/Terr%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!--- ECCC Meteo Stations - Available at https://dd.weather.gc.ca/hydrometric/doc/ -->
	<locationSet id="ECCCStations">
		<csvFile>
			<file>swob-xml_station_list.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%MSC_ID%</id>
			<name>%Name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<z>%Elevation(m)%</z>
			<attribute id="ID" name="Station Code">
				<text>%MSC_ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Province/Territory%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<locationSet id="RIFTStations">
		<csvFile>
			<file>StationsRIFT.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>RIFT_%ID%</id>
			<name>%Name%</name>
			<x>%Long%</x>
			<y>%Lat%</y>
			<z>%Altitude%</z>
			<attribute id="Type">
				<text>%Type%</text>
			</attribute>
			<attribute id="System">
				<text>%System%</text>
			</attribute>
			<attribute id="Weight">
				<number>%Weight%</number>
			</attribute>
			<attribute id="Shift">
				<number>%Shift%</number>
			</attribute>
			<attribute id="Altitude">
				<number>%Altitude%</number>
			</attribute>
		</csvFile>
	</locationSet>
	<locationSet id="RIFT_CLIMATO">
		<locationSetId>RIFTStations</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextEquals id="System" equals="CLIMATO"/>
				<attributeTextEquals id="System" equals="ECCC-AUTO"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="RIFT_ECCC">
		<locationSetId>RIFTStations</locationSetId>
