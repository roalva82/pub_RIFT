	<import>
		<general>
			<importType>NETCDF-CF_GRID</importType>
			<folder>$IMPORT_FOLDER_EO_DATA$\RCM\processed\$REGION$\$PRODUCT$\</folder>
			<fileNamePatternFilter>*$LOOP_LOCATION_ID$.nc</fileNamePatternFilter>
			<fileNameObservationDateTimePattern>'RCM?_'yyyyMMdd'_'HHmmss'?'</fileNameObservationDateTimePattern>
			<deleteImportedFiles>false</deleteImportedFiles>
			<idMapId>IdImportRCM</idMapId>
			<unitConversionsId>ImportUnitConversions</unitConversionsId>
			<importTimeZone>
				<timeZoneName>GMT+0</timeZoneName>
			</importTimeZone>
			<!-- <skipMissingValues>true</skipMissingValues> -->
			<dataFeedId>RCM</dataFeedId>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>ImportRCM</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>Gamma0_HH</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>add originals</readWriteMode>
			<expiryTime unit="day" multiplier="$RCM_EXPIRY_DAYS$"/>
		</timeSeriesSet>
