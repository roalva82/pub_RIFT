	<transformation id="LI-BVA">
		<user>
			<simple>
				<expression>if( (Q_023402_shift-Q_023422-Q_023429)*Weight &gt;0, (Q_023402_shift-Q_023422-Q_023429)*Weight, 0)</expression>
				<coefficientSetFunctions>
					<coefficient id="Weight" value="@Weight@"/>
