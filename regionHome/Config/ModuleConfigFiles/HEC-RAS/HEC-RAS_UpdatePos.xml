<?xml version="1.0" encoding="UTF-8"?>
<transformationModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/transformationModule.xsd" version="1.0">
	<variable>
		<variableId>UpdateRun</variableId>
		<timeSeriesSet>
			<moduleInstanceId>HEC-RAS_Update</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>QR.sim</parameterId>
			<locationId>RIFT_023402</locationId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-48" end="0" startOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>BDH</variableId>
		<timeSeriesSet>
			<moduleInstanceId>ProcessBDH</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>Q.obs</parameterId>
			<locationId>RIFT_023402</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-48" end="0" startOverrulable="true"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>Difference</variableId>
		<timeSeriesSet>
			<moduleInstanceId>HEC-RAS_UpdatePos</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>QR.sim</parameterId>
			<locationId>RIFT_023402</locationId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-48" end="0" startOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</variable>
	<transformation id="Diff">
		<user>
			<simple>
				<expression>BDH-UpdateRun</expression>
				<outputVariable>
					<variableId>Difference</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
</transformationModule>
