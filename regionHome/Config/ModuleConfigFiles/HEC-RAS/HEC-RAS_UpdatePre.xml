	<transformation id="Copy_BDH">
		<user>
			<simple>
				<expression>Q_inputs_1hr_BDH</expression>
				<outputVariable>
					<variableId>Q_inputs_1hr</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="Extrapolate_LI">
		<interpolationSerial>
			<extrapolateConstant>
				<inputVariable>
					<variableId>LI_Estimate</variableId>
				</inputVariable>
				<extrapolateDirection>both</extrapolateDirection>
				<outputVariable>
					<variableId>LI_Estimate_extrapolate</variableId>
				</outputVariable>
			</extrapolateConstant>
		</interpolationSerial>
	</transformation>
	<transformation id="Shift_LI">
		<timeShift>
			<constant>
				<inputVariable>
					<variableId>LI_Estimate_extrapolate</variableId>
				</inputVariable>
				<direction>backward</direction>
				<numberOfTimeSteps>@Shift@</numberOfTimeSteps>
				<outputVariable>
					<variableId>LI_Estimate_shift</variableId>
				</outputVariable>
			</constant>
		</timeShift>
	</transformation>
	<transformation id="Fill_Shift_LI">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>LI_Estimate_shift</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>LI_Estimate_shift</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
