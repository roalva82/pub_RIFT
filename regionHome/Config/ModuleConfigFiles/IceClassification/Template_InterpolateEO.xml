<transformationModule version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.wldelft.nl/fews" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/transformationModule.xsd">
	<variable>
		<variableId>InputGrid</variableId>
		<timeSeriesSet>
			<moduleInstanceId>$MI_IN$</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>$PARAM$</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>			<relativeViewPeriod unit="day" start="-14" end="7"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>OutputGrid</variableId>
		<timeSeriesSet>
			<moduleInstanceId>$MI_OUT$</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>$PARAM$</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<!-- This may need to be updated to below. Changing as it currently causes start up errors -->
			<!--<locationId>$LOOP_LOCATION_ID$_$SOURCE$</locationId>-->
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>			<relativeViewPeriod unit="day" start="-14" end="7"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</variable>
	<transformation id="OutputGrid">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>InputGrid</variableId>
				</inputVariable>
				<searchRadius>50</searchRadius>
				<distanceGeoDatum>$DISTANCEGEODATUM$</distanceGeoDatum>
				<outputVariable>
					<variableId>OutputGrid</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
</transformationModule>
