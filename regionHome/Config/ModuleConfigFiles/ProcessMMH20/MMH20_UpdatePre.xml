				<interpolationType>seriesgeneration</interpolationType>
				<valueOption>normal</valueOption>
				<variogram>
					<type>exponential</type>
					<nugget>0.1</nugget>
					<sill>0.8</sill>
					<range>180</range>
				</variogram>
				<numberOfPoints>8</numberOfPoints>
				<distanceParameters>
					<g1>73</g1>
					<g2>111</g2>
					<g3>0</g3>
					<g4>0</g4>
				</distanceParameters>
				<outputVariable>
