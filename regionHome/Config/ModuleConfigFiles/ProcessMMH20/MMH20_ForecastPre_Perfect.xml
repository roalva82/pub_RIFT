	<transformation id="Interpolate_TMin">
		<interpolationSerial>
			<linear>
				<inputVariable>
					<variableId>Tmin_CLIMATO_3hr</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Tmin_CLIMATO_3hr_interpolated</variableId>
				</outputVariable>
			</linear>
		</interpolationSerial>
	</transformation>
	<transformation id="Tmin_at_sea_level">
		<user>
			<simple>
				<expression>Tmin_CLIMATO_3hr_interpolated - gradient_per_100m * altitude/100</expression>
				<coefficientSetFunctions>
					<coefficient id="altitude" value="@Altitude@"/>
					<coefficient id="gradient_per_100m" value="-0.5"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Tmin_at_sea_level</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="T.obs Min Kriging">
		<interpolationSpatial>
			<kriging>
				<inputVariable>
					<variableId>Tmin_at_sea_level</variableId>
				</inputVariable>
				<interpolationType>seriesgeneration</interpolationType>
				<valueOption>normal</valueOption>
				<variogram>
					<type>exponential</type>
					<nugget>0.1</nugget>
					<sill>1</sill>
					<range>360</range>
				</variogram>
				<numberOfPoints>10</numberOfPoints>
				<distanceParameters>
					<g1>73</g1>
					<g2>111</g2>
					<g3>0</g3>
					<g4>0</g4>
				</distanceParameters>
				<outputVariable>
					<variableId>Tmin_CLIMATO_3hr_kriging_sea_level</variableId>
				</outputVariable>
			</kriging>
		</interpolationSpatial>
	</transformation>
	<transformation id="InterpolateSerial_Tmin">
		<interpolationSpatial>
			<average>
				<inputVariable>
