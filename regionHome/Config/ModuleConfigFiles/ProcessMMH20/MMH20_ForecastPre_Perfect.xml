	<transformation id="Merge P.obs">
		<merge>
			<simple>
				<inputVariable>
					<variableId>P_CLIMATO_1hr_CLIMATO</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>P_CLIMATO_1hr_ECCC</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>P_CLIMATO_1hr</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>	
	<transformation id="P.obs from 1hr to 3hr">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>P_CLIMATO_1hr</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>P_CLIMATO_3hr_temp</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="Merge_P3hr">
		<merge>
			<simple>
				<inputVariable>
					<variableId>P_CLIMATO_3hr</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>P_CLIMATO_3hr_temp</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>P_CLIMATO_3hr</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Interpolate_P">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>P_CLIMATO_3hr</variableId>
				</inputVariable>
				<maxGapLength>8</maxGapLength>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>P_CLIMATO_3hr_interpolated</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="P.obs Kriging">
		<interpolationSpatial>
			<kriging>
				<inputVariable>
					<variableId>P_CLIMATO_3hr_interpolated</variableId>
				</inputVariable>
				<interpolationType>seriesgeneration</interpolationType>
				<valueOption>normal</valueOption>
				<variogram>
					<type>exponential</type>
					<nugget>0.1</nugget>
					<sill>0.8</sill>
					<range>180</range>
				</variogram>
				<numberOfPoints>8</numberOfPoints>
				<distanceParameters>
					<g1>73</g1>
					<g2>111</g2>
					<g3>0</g3>
					<g4>0</g4>
				</distanceParameters>
				<outputVariable>
					<variableId>P_CLIMATO_3hr_kriging_tmp1</variableId>
				</outputVariable>
			</kriging>
		</interpolationSpatial>
	</transformation>
	<transformation id="P.obs check">
		<user>
			<simple>
				<expression>max(P_CLIMATO_3hr_kriging_tmp1,0)</expression>
				<outputVariable>
					<variableId>P_CLIMATO_3hr_kriging</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="InterpolateSerial_PC">
		<interpolationSpatial>
			<average>
				<inputVariable>
					<variableId>P_CLIMATO_3hr_kriging</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>P_CLIMATO_3hr_kriging_Average</variableId>
				</outputVariable>
			</average>
		</interpolationSpatial>
	</transformation>
	<transformation id="T.obs Min from 1hr to 3hr">
		<statisticsSerial>
			<min>
				<inputVariable>
