	<transformation id="TA_daily_basin_max_temp">
		<statisticsSerial>
			<max>
				<inputVariable>
					<variableId>TA_spatial_avg</variableId>
				</inputVariable>
				<validationRule>
					<inputMissingPercentage>30</inputMissingPercentage>
					<outputValueFlag>reliable</outputValueFlag>
				</validationRule>
				<validationRule>
					<inputMissingPercentage>100</inputMissingPercentage>
					<outputValueFlag>missing</outputValueFlag>
