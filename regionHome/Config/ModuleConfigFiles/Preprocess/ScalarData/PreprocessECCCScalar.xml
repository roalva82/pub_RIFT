	<transformation id="TA_hourly_basin_AVG">
		<interpolationSpatial>
			<inverseDistance>
				<inputVariable>
					<variableId>TA_hourly_final</variableId>
				</inputVariable>
				<includeMissingValues>false</includeMissingValues>
				<searchRadius>800000</searchRadius>
				<inverseDistancePower>1</inverseDistancePower>
				<numberOfPoints>4</numberOfPoints>
				<outputVariable>
					<variableId>TA_spatial_avg_temp</variableId>
				</outputVariable>
			</inverseDistance>
		</interpolationSpatial>
