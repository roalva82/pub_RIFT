	<!--Transformations-->
	<transformation id="TA_hourly_sample">
		<sample>
			<nonEquidistant>
				<nonEquidistantInputVariable>
					<variableId>TA_hourly_nonequi</variableId>
				</nonEquidistantInputVariable>
				<interpolationType>linearOrClosest</interpolationType>
				<maxGapLength unit="hour" multiplier="1"/>
